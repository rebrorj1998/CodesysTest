{"payload":{"meta":{"Graph":{"@Type":"81297157","@Value":{"Guid":"(Guid)5ac029bb-c6d5-4e44-b128-d7522798a20f","ParentGuid":"(Guid)9266ed0f-7f1e-4370-8ffa-55b419b1a46d","Name":"(string)FB_SimMotor","Properties":{"@Type":"2c41fa04:IDictionary","@Value":[{"@Key":"(Guid)829a18f2-c514-4f6e-9634-1df173429203","@Value":{"@Type":"829a18f2","@Value":{"ParentObjects":{"@Type":"fa2ee218:IDictionary","@Value":[{"@Key":"(Guid)d9b2b2cc-ea99-4c3b-aa42-1e5c49e65b84","@Value":"(Guid)b3548ec3-0581-4434-939b-078cf6795610"}]}}}}]},"TypeGuid":"(Guid)6f9dac99-8de1-4efc-8465-68ac443b7d08","EmbeddedTypeGuids":{"@Type":"[Guid]","@Value":["(Guid)a9ed5b7e-75c5-4651-af16-d2c27e98cb94","(Guid)3b83b776-fb25-43b8-99f2-3c507c9143fc"]}}},"TypeInfos":{"2c41fa04":"{2c41fa04-1834-41c1-816e-303c7aa2c05b}","81297157":"{81297157-7ec9-45ce-845e-84cab2b88ade}","829a18f2":"{829a18f2-c514-4f6e-9634-1df173429203}","fa2ee218":"{fa2ee218-a39b-4b6d-b249-49dbddbd168a}","Guid":"System.Guid","string":"System.String"}},"object":{"Graph":{"@Type":"6f9dac99","@Value":{"SpecialFunc":{"@Type":"0db3d7bb:Enum","@Value":"None"},"Implementation":{"@Type":"3b83b776","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string)//\tFunction Block: FB_SimMotor()\n//\tDescription: A simulated motor axis to test MC commands \n//\t\t\t\t \n//\tCreated by: Ben Hess\n//\tCreated Date: Tuesday, May 14th, 2019\n\nhomingTimer();\nmovingTimer();\nresetTimer();\noperational := PowerOn;\nEnableState := DriveEnable;\n\nIF AckError THEN\n\t_reset := TRUE;\nEND_IF\n\nIF _reset THEN\n\t//Error := FALSE;\n\tresetTimer.IN := TRUE;\n\tisHomed := FALSE;\n\t_reset := FALSE;\nEND_IF\n\nIF resetTimer.Q THEN\n\tError := FALSE;\n\tresetTimer.IN := FALSE;\nEND_IF\n\nIF simFault THEN \n\tError := TRUE;\n\tisHomed := FALSE;\n\tsimFault := FALSE;\n\tTargetReached := FALSE;\nEND_IF\n\nCASE state OF\n\t0:\t\t// Idle\n\t\t\tIF StartHoming THEN\n\t\t\t\tstate := 1;\n\t\t\tEND_IF\n\t\t\t\n\t\t\tIF MoveAbs THEN\n\t\t\t\tTargetReached := FALSE;\n\t\t\t\tIF targetPosDist > ActPos THEN\n\t\t\t\t\treverseMoveRequired := FALSE;\n\t\t\t\tELSE\n\t\t\t\t\treverseMoveRequired := TRUE;\n\t\t\t\tEND_IF\n\t\t\t\tdistanceToMove := ABS(ActPos - targetPosDist);\n\t\t\t\tMoveAbs := FALSE;\n\t\t\t\tAxisState := eAxisState.DiscreteMotion;\n\t\t\t\tstate := 2;\n\t\t\tEND_IF\n\t\t\t\n\t\t\tIF MoveRel THEN\n\t\t\t\tTargetReached := FALSE;\n\t\t\t\tdistanceToMove := targetPosDist;\n\t\t\t\tstate := 2;\n\t\t\tEND_IF\n\t\t\n\t1:\t\t// Homing\n\t\t\thomingTimer.PT := T#5S;\n\t\t\thomingTimer.IN := TRUE;\n\t\t\n\t\t\tIF homingTimer.Q THEN\n\t\t\t\thomingTimer.IN := FALSE;\n\t\t\t\tActPos := 2.0;\n\t\t\t\tisHomed := TRUE;\n\t\t\t\tstate := 100;\n\t\t\tEND_IF\n\t\t\n\t2: \t\t// Moving to position\n\t\t\tmovingTimer.PT := TO_TIME((distanceToMove * 1/ProfileVelocity) * 1000);\n\t\t\tmovingTimer.IN := TRUE;\n\t\t\tIF reverseMoveRequired THEN\n\t\t\t\tActPos := targetPosDist - ((TO_REAL(movingTimer.ET) * ProfileVelocity)  / 1000) + distanceToMove;\n\t\t\tELSE\n\t\t\t\tActPos := targetPosDist - ((TO_REAL(movingTimer.PT - movingTimer.ET) * ProfileVelocity)  / 1000);\n\t\t\tEND_IF\n\t\t\t\n\t\t\tIF Halt THEN\n\t\t\t\tstate := 100;\n\t\t\t\tAxisState := eAxisState.Standstill;\n\t\t\tEND_IF\n\t\t\tIF movingTimer.Q THEN\n\t\t\t\tmovingTimer.IN := FALSE;\n\t\t\t\tTargetReached := TRUE;\n\t\t\t\tAxisState := eAxisState.Standstill;\n\t\t\t\tstate := 100;\n\t\t\tEND_IF\n\t\t\t\n\t\n\t\n\t3:\t\t// \n\t\n\t\n\t100:\t// Clear/Reset\n\t\t\thomingTimer.IN := FALSE;\n\t\t\tmovingTimer.IN := FALSE;\n\t\t\t\n\t\t\tstate := 0;\n\t\nEND_CASE\n\n\n","LineInfoPersistence":"(string)5ac029bb-c6d5-4e44-b128-d7522798a20f_Impl_LineIds"}}}},"Interface":{"@Type":"a9ed5b7e","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string)FUNCTION_BLOCK FB_SimMotor EXTENDS Axis_REF\nVAR_INPUT\n\tsimFault : BOOL;\nEND_VAR\nVAR_OUTPUT\nEND_VAR\nVAR\n\thomingTimer : Standard.TON;\n\tmovingTimer : Standard.TON;\n\tresetTimer : Standard.TON := (PT := T#500ms);\n\tstate : INT;\n\thomingState : INT;\n\tmoveAbsState : INT;\n\tmoveRelState : INT;\n\t_reset : BOOL;\n\tdistanceToMove : REAL;\n\treverseMoveRequired : BOOL;\n\t\nEND_VAR\n","LineInfoPersistence":"(string)5ac029bb-c6d5-4e44-b128-d7522798a20f_Decl_LineIds"}}}},"UniqueIdGenerator":"(string)148","POULevel":{"@Type":"8e575c5b:Enum","@Value":"Standard"},"ChildObjectGuids":{"@Type":"ArrayList:IList","@Value":[]},"AddAttributeSubsequent":"(bool)False"}},"TypeInfos":{"0db3d7bb":"{0db3d7bb-cde0-4416-9a7b-ce49a0124323}","3b83b776":"{3b83b776-fb25-43b8-99f2-3c507c9143fc}","6f9dac99":"{6f9dac99-8de1-4efc-8465-68ac443b7d08}","8e575c5b":"{8e575c5b-1d37-49c6-941b-5c0ec7874787}","a9ed5b7e":"{a9ed5b7e-75c5-4651-af16-d2c27e98cb94}","ArrayList":"System.Collections.ArrayList","bool":"System.Boolean","f3878285":"{f3878285-8e4f-490b-bb1b-9acbb7eb04db}","string":"System.String"}}},"FormatVersion":"1.0"}